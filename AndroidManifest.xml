<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.wisc.perperkeyboard"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="edu.wisc.perperkeyboard.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
			 <intent-filter>
                <action android:name="android.intent.action.MAIN" />
 				 <category android:name="android.intent.category.LAUNCHER" />
             
            </intent-filter>
        </activity>
        <activity
            android:name="edu.wisc.perperkeyboard.Testing"
            android:label="@string/title_activity_testing"
            android:parentActivityName="edu.wisc.perperkeyboard.MainActivity"
            android:screenOrientation="landscape"
             android:windowSoftInputMode="adjustResize"  >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.wisc.perperkeyboard.MainActivity" />
            
        </activity>
        <activity
            android:name="edu.wisc.perperkeyboard.TestingActivity"
            android:label="@string/title_activity_testing" >
        </activity>
    </application>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

</manifest>
